{
  "name": "nextjs-starter",
  "private": true,
  "scripts": {
    "build": "next build",
    "commit": "gacp",
    "dev": "next dev",
    "lint": "next lint",
    "start": "next start",
    "test": "jest --watch",
    "test-coverage": "jest --coverage",
    "test-coverage-view": "http-server ./coverage -p 9999",
    "prepare": "husky install"
  },
  "dependencies": {
    "next": "12.0.8",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.4.0",
    "@types/node": "17.0.10",
    "@types/react": "17.0.38",
    "eslint": "8.7.0",
    "eslint-config-next": "12.0.8",
    "gacp": "^2.10.2",
    "husky": "^7.0.4",
    "jest": "^27.5.0",
    "lint-staged": "^12.3.3",
    "prettier": "^2.5.1",
    "typescript": "4.5.4"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  }
}
